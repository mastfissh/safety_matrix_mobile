{
  "expo": {
    "name": "combos",
    "slug": "safety-matrix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "userInterfaceStyle": "light",
      "supportsTablet": true,
      "bundleIdentifier": "com.mastfish.psychcombo"
    },
    "android": {
      "userInterfaceStyle": "light",
      "package": "com.mastfish.psychcombo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {

        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images",
            "./assets/images/_astro"
          ]
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ec3bb2ae-1dda-4647-947e-f0c9485c7acf"
      }
    },
    "owner": "mastfish-org"
  }
}
